#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pankajnegi/Desktop/awsdevday/bedrock_agent_cdk/node_modules/.pnpm/aws-cdk-lib@2.161.1_constructs@10.4.2/node_modules/aws-cdk-lib/node_modules/semver/bin/node_modules:/Users/pankajnegi/Desktop/awsdevday/bedrock_agent_cdk/node_modules/.pnpm/aws-cdk-lib@2.161.1_constructs@10.4.2/node_modules/aws-cdk-lib/node_modules/semver/node_modules:/Users/pankajnegi/Desktop/awsdevday/bedrock_agent_cdk/node_modules/.pnpm/aws-cdk-lib@2.161.1_constructs@10.4.2/node_modules/aws-cdk-lib/node_modules:/Users/pankajnegi/Desktop/awsdevday/bedrock_agent_cdk/node_modules/.pnpm/aws-cdk-lib@2.161.1_constructs@10.4.2/node_modules:/Users/pankajnegi/Desktop/awsdevday/bedrock_agent_cdk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pankajnegi/Desktop/awsdevday/bedrock_agent_cdk/node_modules/.pnpm/aws-cdk-lib@2.161.1_constructs@10.4.2/node_modules/aws-cdk-lib/node_modules/semver/bin/node_modules:/Users/pankajnegi/Desktop/awsdevday/bedrock_agent_cdk/node_modules/.pnpm/aws-cdk-lib@2.161.1_constructs@10.4.2/node_modules/aws-cdk-lib/node_modules/semver/node_modules:/Users/pankajnegi/Desktop/awsdevday/bedrock_agent_cdk/node_modules/.pnpm/aws-cdk-lib@2.161.1_constructs@10.4.2/node_modules/aws-cdk-lib/node_modules:/Users/pankajnegi/Desktop/awsdevday/bedrock_agent_cdk/node_modules/.pnpm/aws-cdk-lib@2.161.1_constructs@10.4.2/node_modules:/Users/pankajnegi/Desktop/awsdevday/bedrock_agent_cdk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../semver/bin/semver.js" "$@"
fi
