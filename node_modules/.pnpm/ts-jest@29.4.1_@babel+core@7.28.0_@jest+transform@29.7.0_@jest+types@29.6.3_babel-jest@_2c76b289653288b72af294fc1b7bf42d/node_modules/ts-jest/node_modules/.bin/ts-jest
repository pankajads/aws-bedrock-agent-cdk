else
  export NODE_PATH="/Users/pankajnegi/Desktop/awsdevday/bedrock_agent_cdk/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.0_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_2c76b289653288b72af294fc1b7bf42d/node_modules/ts-jest/node_modules:/Users/pankajnegi/Desktop/awsdevday/bedrock_agent_cdk/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.0_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_2c76b289653288b72af294fc1b7bf42d/node_modules:/Users/pankajnegi/Desktop/awsdevday/bedrock_agent_cdk/node_modules/.pnpm/node_modules:$NODE_PATH"
